      $('.myresult a').attr('href',`/pages/result4?id=${id}`);
      $('.BannerUrl').attr('href',`/pages/result4?id=${id}`);
    }
    $('.myresult').addClass('d-inline-block');
    $('.myresult').removeClass('d-none');
    $('.take-hair-sticky-bototm .st-text').text('My Result');
    $('.take-hair-sticky-bototm .btn').attr('href','/pages/result4');
    $(".female_button").addClass("already_result");
  }else if(id && user) {
    $('.myresult').removeClass('d-inline-block');
    $('.myresult').addClass('d-none');
    $('.reorder').show();
    $('[homeReorder]').attr("href", `https://form.traya.health/recommendedcart/${caseid}`);
    $(".female_button").removeClass("already_result");
  } else {
    $('.myresult').addClass('d-none');
    $('.myresult').removeClass('d-inline-block');
    $(".female_button").removeClass("already_result");
  }


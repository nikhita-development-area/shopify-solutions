$(document).ready(function(){
  var id = localStorage.getItem('rid');
  var user_gender = localStorage.getItem('user-gender');
  var Synthetic_ID = getCookie("Synthetic_ID");
  let form_status = getCookie("form_status");
  let user = localStorage.getItem('user');
  let caseid = localStorage.getItem('caseId');
  if(Synthetic_ID && (form_status == "filled")){
  id = Synthetic_ID;
  }else{
  id = '';
  }
  if(user_gender == null) {
    user_gender = getCookie("gender")
  }
  if (id && (user == undefined || user == null)) {
    $('.header__icons .assessment-btn').removeClass('d-lg-inline-block').addClass('d-none');
    if(user_gender == 'female' || user_gender == 'F'){
      $('.myresult a').attr('href',`/pages/female-result?id=${id}`);
      $('.BannerUrl').attr('href',`/pages/female-result?id=${id}`);
    }

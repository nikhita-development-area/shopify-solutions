<script>

window.onscroll = function() {myFunction()};

var header = document.getElementById("bannertnq");

function myFunction() {
  if ($(window).scrollTop() >= 50) {
    header.classList.add("sticky");
  } else {
    header.classList.remove("sticky");
  }
}
</script>
<script>

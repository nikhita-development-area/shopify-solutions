    var cusname = CUSName.replace(/^\s+|\s+$/gm,'');
    var OrderID = Order.replace ( /[^\d.]/g, '' );
    var ID = parseInt(OrderID);
    $('[chekout-order-id]').text('Order ID #' + order__id);
    $('[chekout-order-amount]').text('Amount : â‚¹' +order_res_amount);
    $('[chekout-order-name]').text(cusname);
  });

  .tooltip {
    position: relative;
    display: inline-block;
    opacity: 1;
    margin-right: 0;
    margin-left: auto;
    z-index: 2;
  }
  .tooltip .tooltiptext {
    visibility: hidden;
    font-weight:normal !important;
    color: #000 !important;
    text-align: left !important;
    border-radius: 6px !important;
    padding: 5px 0;
    z-index: 1 !important;
    font-size: 12px !important;
    line-height: 16px !important;
    background: rgb(255, 255, 255);
    box-shadow: rgba(0, 0, 0, 0.05) 0px 0px 20px 0px;
    position: absolute;
    top: -85px;
    width: 225px;
    left: -200px;
    border-radius: 8px;
    padding: 8px 12px;
  }
  .tooltip .tooltiptext::after {
    content: "";
    display: inline-block;
    height: 16px;
    width: 16px;
    border-width: 8px;
    border-style: solid;
    border-color: white transparent transparent;
    border-image: initial;
    position: absolute;
    top: 80px;
    right: 10px;
  }
  .tooltip:hover .tooltiptext {
    visibility: visible;
  }

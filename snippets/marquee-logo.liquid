{% style %}
* {
  box-sizing: border-box;
}
body {
  margin: 0;
}
.marquee-content {
  display: flex;
  animation: scrolling 20s linear infinite;
}
.marquee-item {
  flex: 0 0 13vw;
  margin: 0 1vw;
}
.marquee-item img {
  display: block;
  width: 100%;
}
@keyframes scrolling {
  0% { transform: translateX(0); }
  100% { transform: translatex(-144vw); }
}
@media only screen and (min-width: 767.98px) {
  .marqueeMain{
    border-bottom: 1px solid rgb(64 211 185 / 30%);
  }
  .marquee-item img {
    width: 160px;
  }
}
@media only screen and (max-width: 767px) {
  .marquee-item {
    flex: 0 0 40%;
  }
  .marquee {
    overflow: hidden;
    border-bottom: 1px solid rgb(64 211 185 / 30%);
  }
  .marquee-item img {
    width: 115px;
  }
}
{% endstyle %}
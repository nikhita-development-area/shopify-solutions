{% if template == 'index.login' %}
  {% assign page__hndl = 'home' %}
{% elsif template == 'index' %}
  {% assign page__hndl = 'home' %}
{% else %}
  {% assign page__hndl = page.handle %}
{% endif %}

{{ 'section-image-banner.css' | asset_url | stylesheet_tag }}
{{ 'component-slider.css' | asset_url | stylesheet_tag }}
{{ 'component-slideshow.css' | asset_url | stylesheet_tag }}


{%- if section.settings.slide_height == 'adapt_image' and section.blocks.first.settings.image != blank -%}
  {%- style -%}
  @media screen and (max-width: 749px) {
    #Slider-{{ section.id }}::before,
    #Slider-{{ section.id }} .media::before,
    #Slider-{{ section.id }}:not(.banner--mobile-bottom) .banner__content::before {
      padding-bottom: {{ 1 | divided_by: section.blocks.first.settings.image.aspect_ratio | times: 100 }}%;
/*       content: ''; */
      display: block;
    }
     .Desktop-image{
      display: none;
    }
    .Mobile-image{
      display: block;
      position:relative;
    }
  }

  @media screen and (min-width: 750px) {
    #Slider-template--15985733304498__7c4158ab-5b92-41d5-aa2c-227050df3e46::before, #Slider-template--15985733304498__7c4158ab-5b92-41d5-aa2c-227050df3e46 .media::before {
    padding-bottom: 45.54537885095753% !important;
}
#Slider-template--15976672788658__1663823771acb5bafd::before, #Slider-template--15976672788658__1663823771acb5bafd .media::before{
   padding-bottom: 45.54537885095753% !important;
}
    #Slider-{{ section.id }}::before,
    #Slider-{{ section.id }} .media::before {
      padding-bottom: {{ 1 | divided_by: section.blocks.first.settings.image.aspect_ratio | times: 100 }}%;
      content: '';
      display: block;
    }
    .Mobile-image{
      display: none;
    }
    .Desktop-image{
      display: block;
    }
  }
  {%- endstyle -%}
{%- endif -%}
<!--   <a class=" BannerUrl" href="https://form.traya.health/questions?cohort=1&location=HomeSlideshow&page=home"> -->
<slideshow-component class=" slider-mobile-gutter{% if section.settings.layout == 'grid' %} page-width{% endif %}{% if section.settings.show_text_below %} mobile-text-below{% endif %}" role="region" aria-roledescription="{{ 'sections.slideshow.carousel' | t }}" aria-label="{{ section.settings.accessibility_info | escape }}">
  {%- if section.settings.auto_rotate and section.blocks.size > 1 -%}
    <div class="slideshow__controls slideshow__controls--top slider-buttons no-js-hidden{% if section.settings.show_text_below %} slideshow__controls--border-radius-mobile{% endif %}">
      <button type="button" class="slider-button slider-button--prev" name="previous" aria-label="{{ 'sections.slideshow.previous_slideshow' | t }}" aria-controls="Slider-{{ section.id }}">{% render 'icon-caret' %}</button>
      <div class="slider-counter slider-counter--{{ section.settings.slider_visual }}{% if section.settings.slider_visual == 'counter' or section.settings.slider_visual == 'numbers' %} caption{% endif %}">
        {%- if section.settings.slider_visual == 'counter' -%}
          <span class="slider-counter--current">1</span>
          <span aria-hidden="true"> / </span>
          <span class="visually-hidden">{{ 'general.slider.of' | t }}</span>
          <span class="slider-counter--total">{{ section.blocks.size }}</span>
        {%- else-%}
          <div class="slideshow__control-wrapper">
            {%- for block in section.blocks -%}
              <button class="slider-counter__link slider-counter__link--{{ section.settings.slider_visual }} link" aria-label="{{ 'sections.slideshow.load_slide' | t }} {{ forloop.index }} {{ 'general.slider.of' | t }} {{ forloop.length }}" aria-controls="Slider-{{ section.id }}">
                {%- if section.settings.slider_visual == 'numbers' -%}{{ forloop.index }}{% else %}<span class="dot"></span>{%- endif -%}
              </button>
            {%- endfor -%}
          </div>
        {%- endif -%}
      </div>
      <button type="button" class="slider-button slider-button--next" name="next" aria-label="{{ 'sections.slideshow.next_slideshow' | t }}" aria-controls="Slider-{{ section.id }}">{% render 'icon-caret' %}</button>

      {%- if section.settings.auto_rotate -%}
        <button type="button" class="slideshow__autoplay slider-button no-js-hidden{% if section.settings.auto_rotate == false %} slideshow__autoplay--paused{% endif %}" aria-label="{{ 'sections.slideshow.pause_slideshow' | t }}">
          {%- render 'icon-pause' -%}
          {%- render 'icon-play' -%}
        </button>
      {%- endif -%}
    </div>
    <noscript>
      <div class="slider-buttons">
        <div class="slider-counter">
          {%- for block in section.blocks -%}
            <a href="#Slide-{{ section.id }}-{{ forloop.index }}" class="slider-counter__link link" aria-label="{{ 'sections.slideshow.load_slide' | t }} {{ forloop.index }} {{ 'general.slider.of' | t }} {{ forloop.length }}">
              {{ forloop.index }}
            </a>
          {%- endfor -%}
        </div>
      </div>
    </noscript>
  {%- endif -%}

  <div class="slideshow banner banner--{{ section.settings.slide_height }} grid grid--1-col slider slider--everywhere{% if section.settings.show_text_below %} banner--mobile-bottom{% endif %}{% if section.blocks.first.settings.image == blank %} slideshow--placeholder{% endif %}"
    id="Slider-{{ section.id }}"
    aria-live="polite"
    aria-atomic="true"
    data-autoplay="{{ section.settings.auto_rotate }}"
    data-speed="{{ section.settings.change_slides_speed }}"
  >
    {%- for block in section.blocks -%}
      <style>
        #Slide-{{ section.id }}-{{ forloop.index }} .banner__media::after {
          opacity: {{ block.settings.image_overlay_opacity | divided_by: 100.0 }};
        }
      </style>
      <div class="slideshow__slide grid__item grid--1-col slider__slide"
        id="Slide-{{ section.id }}-{{ forloop.index }}"
        {{ block.shopify_attributes }}
        role="group"
        aria-roledescription="{{ 'sections.slideshow.slide' | t }}"
        aria-label="{{ forloop.index }} {{ 'general.slider.of' | t }} {{ forloop.length }}"
        tabindex="-1"
      >
        <div class="round-slide_img slideshow__media banner__media media Desktop-image{% if block.settings.image == blank %} placeholder{% endif %}">
          {%- if block.settings.image -%}
            <img
              srcset="{%- if block.settings.image.width >= 375 -%}{{ block.settings.image | image_url: width: 375 }} 375w,{%- endif -%}
              {%- if block.settings.image.width >= 550 -%}{{ block.settings.image | image_url: width: 550 }} 550w,{%- endif -%}
              {%- if block.settings.image.width >= 750 -%}{{ block.settings.image | image_url: width: 750 }} 750w,{%- endif -%}
              {%- if block.settings.image.width >= 1100 -%}{{ block.settings.image | image_url: width: 1100 }} 1100w,{%- endif -%}
              {%- if block.settings.image.width >= 1500 -%}{{ block.settings.image | image_url: width: 1500 }} 1500w,{%- endif -%}
              {%- if block.settings.image.width >= 1780 -%}{{ block.settings.image | image_url: width: 1780 }} 1780w,{%- endif -%}
              {%- if block.settings.image.width >= 2000 -%}{{ block.settings.image | image_url: width: 2000 }} 2000w,{%- endif -%}
              {%- if block.settings.image.width >= 3000 -%}{{ block.settings.image | image_url: width: 3000 }} 3000w,{%- endif -%}
              {%- if block.settings.image.width >= 3840 -%}{{ block.settings.image | image_url: width: 3840 }} 3840w,{%- endif -%}
              {{ block.settings.image | image_url }} {{ block.settings.image.width }}w"
              sizes="100vw"
              src="{{ block.settings.image | image_url: width: 1500 }}"
              loading="lazy"
              alt="{{ block.settings.image.alt | escape }}"
              width="{{ block.settings.image.width }}"
              height="{{ block.settings.image.width | divided_by: block.settings.image.aspect_ratio | round }}"
            >
          {%- else -%}
            {{ 'lifestyle-2' | placeholder_svg_tag: 'placeholder-svg' }}
          {%- endif -%}
        </div>
<!--       Mobile-image -->
         <div class="round-slide_img slideshow__media banner__media media Mobile-image{% if block.settings.mbl_image == blank %} placeholder{% endif %}">
          {%- if block.settings.mbl_image -%}
            <img
              srcset="{%- if block.settings.mbl_image.width >= 375 -%}{{ block.settings.mbl_image | image_url: width: 375 }} 375w,{%- endif -%}
              {%- if block.settings.mbl_image.width >= 550 -%}{{ block.settings.mbl_image | image_url: width: 550 }} 550w,{%- endif -%}
              {%- if block.settings.mbl_image.width >= 750 -%}{{ block.settings.mbl_image | image_url: width: 750 }} 750w,{%- endif -%}
              {%- if block.settings.mbl_image.width >= 1100 -%}{{ block.settings.mbl_image | image_url: width: 1100 }} 1100w,{%- endif -%}
              {%- if block.settings.mbl_image.width >= 1500 -%}{{ block.settings.mbl_image | image_url: width: 1500 }} 1500w,{%- endif -%}
              {%- if block.settings.mbl_image.width >= 1780 -%}{{ block.settings.mbl_image | image_url: width: 1780 }} 1780w,{%- endif -%}
              {%- if block.settings.mbl_image.width >= 2000 -%}{{ block.settings.mbl_image | image_url: width: 2000 }} 2000w,{%- endif -%}
              {%- if block.settings.mbl_image.width >= 3000 -%}{{ block.settings.mbl_image | image_url: width: 3000 }} 3000w,{%- endif -%}
              {%- if block.settings.mbl_image.width >= 3840 -%}{{ block.settings.mbl_image | image_url: width: 3840 }} 3840w,{%- endif -%}
              {{ block.settings.mbl_image | image_url }} {{ block.settings.mbl_image.width }}w"
              sizes="100vw"
              src="{{ block.settings.mbl_image | image_url: width: 1500 }}"
              loading="lazy"
              alt="{{ block.settings.mbl_image.alt | escape }}"
              width="{{ block.settings.mbl_image.width }}"
              height="{{ block.settings.mbl_image.width | divided_by: block.settings.mbl_image.aspect_ratio | round }}"
            >
          {%- else -%}
            {{ 'lifestyle-2' | placeholder_svg_tag: 'placeholder-svg' }}
          {%- endif -%}
        </div>
<!--       <a class="BannerUrl" href="https://form.traya.health/questions?cohort=1&location=HomeSlideshow&page=home"> -->
        <div class="icon-with-slider slideshow__text-wrapper  {% if page.handle contains 'transplant-thank-you' %} thank-banner__content {% endif %} banner__content banner__content--{{ block.settings.box_align }} {% unless page.handle == 'transplant' or page.handle == 'femalev1' or page.handle == 'female' or page.handle == 'new-female' %}page-width{% endunless %}{% if block.settings.show_text_box == false %} banner--desktop-transparent{% endif %}">
          <div class="{% if page.handle == 'transplant' %}col-lg-5 col-md-7 bg-white{% else %}slideshow__text banner__box  {% if page.handle == 'transplant-thank-you' %}thank-content-container {% else %}content-container {% endif %}content-container--full-width-mobile color-{{ block.settings.color_scheme }} gradient slideshow__text--{{ block.settings.text_alignment }} slideshow__text-mobile--{{ block.settings.text_alignment_mobile }}{% endif %}">
            <div class="{% if template.suffix == 'indians-trust-traya' %}bg-white p-md-5 px-3 py-4{% else %}text-section 1{% if page.handle == 'transplant' %} px-md-4 px-2 py-5{% endif %}{% endif %}">
           {%- if block.settings.heading != blank -%}
              <h2 class="{% if page.handle == 'femaleoo' %}femalev1-banner__heading mb-3 mb-md-4{% else %}banner__heading{% endif %} {{ block.settings.heading_size }} text_Fredoka">{{ block.settings.heading }}</h2>
            {%- endif -%}
              <div class="banner__text mb-0{% if template == 'index.login' %} mb-0 pb-0 mt-md-4{% elsif template.suffix == 'indians-trust-traya' %} pb-2 bdr-bottom{% endif %}" {{ block.shopify_attributes }}>
                {% if template == 'index.login' %}
                  <span class="d-none d-md-block second__text pb-0">93% Of Customers Who Stayed Regular With <br>Their Regime Saw Hair Growth Results.</span>
                  <span class="d-md-none second__text pb-0">93% Of Customers Who Stayed Regular With Their Regime Saw Hair Growth Results.</span>
                {% else %}
                {%- if block.settings.subheading != blank -%}
                  <span{% if page.handle == 'transplant' %} class="font-fredoka"{% elsif template.suffix == 'indians-trust-traya' %} class="font-fredoka pb-0"{% endif %}>{{ block.settings.subheading }}</span>
                {%- endif -%}
                {% endif %}
              </div>
      
          {%- if block.settings.details != blank -%}
            <div class="mt-0 mb-2 details-text">
            <span>{{ block.settings.details  }}</span>
            </div>
          {%- endif -%}
         {%- if block.settings.imgicon != blank -%}
            <div class="mt-0 mb-3 imgicon">
            <img src="{{ block.settings.imgicon | img_url:'350x' }}">
            </div>
          {%- endif -%}
          {%- if block.settings.subheading-2 != blank -%}
              <div class="{% if page.handle == 'femaleoo' %}femalev1-sub-banner__text{% else %}sub-banner__text{% endif %} mt-1{% if page.handle == 'all' %} d-none d-md-block{% endif %}" {{ block.shopify_attributes }}>
                <span class="l-0">{{ block.settings.subheading-2 }}</span>
              </div>
            {%- endif -%}
      
                  {%- if block.settings.button_label != blank -%}
                    <div class="banner__buttons singlebutton">
                      <a id="{% if page.handle == 'female' %}female-community-button{% elsif page.handle == 'femalev1' %}femalev1-hair-test-button2{% else %}hairtestcta{% endif %}"{% if block.settings.link %} href="{{ block.settings.link }}"{% else %} role="link" aria-disabled="true"{% endif %} class="hair-form-actn-link btn btn-dark{% if page.handle == 'femaleoo' %} f-hair-test-btn btn-hair-test position-relative mt-4{% elsif page.handle == 'homev5' %} homev5CTA{% endif %}">{{ block.settings.button_label | escape }} </a>
                      
                      {% comment %}
                      <span class="myresult d-none{% if page.handle == 'femaleoo' %} mt-4{% endif %}">
                            {% if page.handle == 'home-v3' %}
                            <a id="myPlanHome" href="/pages/result4-new" class="btn btn-dark mt-4 mt-md-0">My suggested Plan</a>
                            {% elsif page.handle == 'homev5'%}
                            <a id="myPlanHome" href="/pages/result-hi" class="btn btn-dark mt-4 mt-md-0">आपका Traya प्लान</a>
                            {% else %}
                            <a id="myPlanHome" href="/pages/result4" class="btn btn-dark{% if template == 'index' or page.handle == 'homev1' %} rcmd_plan_btn mt-4 mt-md-0{% endif %}{% if page.handle == 'femaleoo' %} btn-hair-test{% endif %}">My Recommended Plan</a>
                            {% endif %}
                        </span>
                    {% endcomment %}
                    </div>
                  {%- endif -%}
          <div class="getyour">
            <svg width="20" height="20" viewBox="0 0 70 87" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M69.4 86.75L6.31066e-06 86.75L34.6889 41.64L69.4 86.75Z" fill="#414042"/>
            <path d="M69.4 45.1099L6.31066e-06 45.1099L34.6889 -9.21456e-05L69.4 45.1099Z" fill="#414042"/>
            </svg>

            <p>Get your queries answered</p>  
          </div>
      {%- if page.handle == 'all' and block.settings.subheading-2 != blank -%}
              <div class="sub-banner__text mt-1  d-md-none" {{ block.shopify_attributes }}>
                <span>{{ block.settings.subheading-2  }}</span>
              </div>
            {%- endif -%}
          </div>
            {% if template == 'index.login' %}
            <div class="btn-section mt-md-4">
              <div class="mb-4 mb-md-5 pb-2 pb-md-0 d-none" kit-status>
                <div class="position-relative">
                  <img src="https://cdn.shopify.com/s/files/1/0100/1622/7394/files/Rectangle_5790_fc0dcca3-d881-448e-9db9-541bf51231f6.png" class="d-none d-md-block" width="70%">
                  <img src="https://cdn.shopify.com/s/files/1/0100/1622/7394/files/Rectangle_5790_fc0dcca3-d881-448e-9db9-541bf51231f6.png" class="d-md-none">
                  <p class="kit_purchase font-weight-bold" kit__status>Your Kit is finished</p>
                </div>
              </div>
              <div class="btns d-flex mt-md-5">
                <a id="homepage" reorder__btn href=" " class="btn btn-dark hair-form-actn-link reorder_btn mr-md-4 d-none" style="pointer-events: auto;">Reorder</a>
                <span class="talk_to_coach_btn ml-0 ml-md-0"><a id="myPlanHome" talk_to_coach="" href="https://form.traya.health/pages/reschedule-slot/c9beed25-31ab-4710-8c5e-67a38a1697f7" class="btn btn-dark coach_btn invisible">Talk to coach</a></span>
              </div>
            </div>
          {% endif %}
          </div>
        </div>
        {% if page.handle == 'about-us' %}
        <div class="d-md-none trust-badge-image mt-4">
        <img src="https://cdn.shopify.com/s/files/1/0100/1622/7394/files/HEA_Stamp_e37982ea-f9e5-4b6d-b7f8-114dcf1cc05c_140x.png">
        </div>
        {% endif %}
      </div>
<!--       </a> -->
    {%- endfor -%}
  </div>

  {%- if section.blocks.size > 1 and section.settings.auto_rotate == false -%}
    <div class="slideshow__controls slider-buttons no-js-hidden{% if section.settings.show_text_below %} slideshow__controls--border-radius-mobile{% endif %}">
      <button type="button" class="slider-button slider-button--prev" name="previous" aria-label="{{ 'sections.slideshow.previous_slideshow' | t }}" aria-controls="Slider-{{ section.id }}">{% render 'icon-caret' %}</button>
      <div class="slider-counter slider-counter--{{ section.settings.slider_visual }}{% if section.settings.slider_visual == 'counter' or section.settings.slider_visual == 'numbers' %} caption{% endif %}">
        {%- if section.settings.slider_visual == 'counter' -%}
          <span class="slider-counter--current">1</span>
          <span aria-hidden="true"> / </span>
          <span class="visually-hidden">{{ 'general.slider.of' | t }}</span>
          <span class="slider-counter--total">{{ section.blocks.size }}</span>
        {%- else-%}
          <div class="slideshow__control-wrapper">
            {%- for block in section.blocks -%}
              <button class="slider-counter__link slider-counter__link--{{ section.settings.slider_visual }} link" aria-label="{{ 'sections.slideshow.load_slide' | t }} {{ forloop.index }} {{ 'general.slider.of' | t }} {{ forloop.length }}" aria-controls="Slider-{{ section.id }}">
                {%- if section.settings.slider_visual == 'numbers' -%}{{ forloop.index }}{% else %}<span class="dot"></span>{%- endif -%}
              </button>
            {%- endfor -%}
          </div>
        {%- endif -%}
      </div>
      <button type="button" class="slider-button slider-button--next" name="next" aria-label="{{ 'sections.slideshow.next_slideshow' | t }}" aria-controls="Slider-{{ section.id }}">{% render 'icon-caret' %}</button>
      {%- if section.settings.auto_rotate -%}
        <button type="button" class="slideshow__autoplay slider-button no-js-hidden{% if section.settings.auto_rotate == false %} slideshow__autoplay--paused{% endif %}" aria-label="{{ 'sections.slideshow.pause_slideshow' | t }}">
          {%- render 'icon-pause' -%}
          {%- render 'icon-play' -%}
        </button>
      {%- endif -%}
    </div>
    <noscript>
      <div class="slider-buttons">
        <div class="slider-counter">
          {%- for block in section.blocks -%}
            <a href="#Slide-{{ section.id }}-{{ forloop.index }}" class="slider-counter__link link" aria-label="{{ 'sections.slideshow.load_slide' | t }} {{ forloop.index }} {{ 'general.slider.of' | t }} {{ forloop.length }}">
              {{ forloop.index }}
            </a>
          {%- endfor -%}
        </div>
      </div>
    </noscript>
  {%- endif -%}
</slideshow-component>

          {% endif %}

          <div class="position-absolute vertical-paragraph">
            {% if forloop.index == 1 %}
              <p class="{% unless page.handle == 'female' or page.handle == 'femalev2' %}d-md-none{% endunless %} position-absolute">*As Per An Internal Study Conducted In December 2022</p>
            {% endif %}
          </div>
        </div>
        
        <div class="d-flex flex-column p-3 pb-0 p-md-0 position-{{ block.settings.content_position }} banner-content banner-{{ forloop.index }}">


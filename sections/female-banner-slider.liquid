              <a id="female-hair-test-button2-scroll{{ forloop.index }}" href="{{ block.settings.button_url }}" class="female_button btn-dark d-flex justify-content-center align-items-center font-weight-bold">{% if page.handle == 'female' %}TAKE FREE HAIR TEST{% else %}{{ block.settings.button_text }}{% endif %}<sup>TM</sup></a>

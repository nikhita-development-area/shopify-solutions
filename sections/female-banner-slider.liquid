  {% if section.blocks.size > 1 %}
    <div class="banner-slider-nav position-absolute d-flex justify-content-between w-100">
      <div class="banner-slider-prev px-1 px-md-3">
        <svg xmlns="http://www.w3.org/2000/svg" width="20" height="40" viewBox="0 0 85 139" fill="none">
          <path d="M73.5 12L16 69.5L73.5 127" stroke="#EEEEEE" stroke-width="22.0833" stroke-linecap="round"/>
        </svg>
      </div>
      <div class="banner-slider-next px-1 px-md-3">
        <svg xmlns="http://www.w3.org/2000/svg" width="20" height="40" viewBox="0 0 86 139" fill="none">
          <path d="M12 12L69.5 69.5L12 127" stroke="#EEEEEE" stroke-width="22.0833" stroke-linecap="round"/>
        </svg>
      </div>
    </div>
  {% endif %}

  <div class="banner-dots d-md-none"></div> 
</div>

<style>      
  {% if page.handle contains 'female' %}
    #myHeader {background: #414042;}
  {% endif %}

  .banner-bg {margin-top: 40px;}
  .banner-dots .slick-dots li button {display: none;}
  .banner-dots .slick-dots li {background: #E5E1E1; border-radius: 50px; height: 10px; width: 10px;}
  .banner-dots .slick-dots li.slick-active {border: 2px solid #404142;}
  .banner-slider-nav {top: 50%;}
  .banner-slider-prev, .banner-slider-next {cursor: pointer;}
  .banner-content a.female_button.already_result {background: #fff; border: 1px solid #414042; color: #414042;}
  .banner-content a.female_button.already_result:hover{background: #414042; border: 1px solid #414042;color: #fff;}
  .vertical-paragraph {bottom: 0; right: 15px; transform-origin: top right; transform: translate(-50%, 0) rotate(-90deg); white-space: nowrap;}
  .vertical-paragraph p {font-size: 10px; line-height: 14px; letter-spacing: 0em; opacity: 60%;}

  .banner-content h4 {font-size: 20px; line-height: 30px; letter-spacing: 0em;}
  .banner-1 h4{font-weight: 400 !important;}
  .banner-content .text3 {line-height: 30px; letter-spacing: 0em; font-size: 18px;}
  .banner-content h2 {letter-spacing: 0.015em; font-size: 50px;}
  .female .banner-1 h2{color: #CB7364;}
  .banner-content h5 {font-size: 20px; line-height: 28px; letter-spacing: 0em;}
  .female .banner-content h5{color: #CB7364;}
  .female .banner-content .font-weight-bold span {color: #414042; font-weight: 800;}
  .banner-content a {width: 301px; height: 47px; border-radius: 9px; font-size: 18px; letter-spacing: 0em; text-transform: uppercase;}
  .icons p {font-size: 14px;}
  .female-customers-content-main {display: flex; gap: 5px;}
  .banner-1 .female-customers-content-main { margin-top: 10px;}
  .female-customers-content {display: flex; align-items: center; gap: 10px;}
  /* .female-customers-content:first-child{padding-right:10px;border-right: 1px solid #B5B3B7;} */
  .female-customers-text {font-family: Nunito Sans;font-size: 20px;font-weight: 700;}

  @media (min-width: 769px) {    
    {% for block in section.blocks %}
    .banner-content {position: absolute; margin-left: 80px;}
    {% endfor %}
    .position-top-left {top: 0; left: 0;}
    .position-top-center {top: 0; left: 50%; transform: translateX(-50%);}
    .position-top-right {top: 0; right: 0;}
    .position-middle-left {top: 50%; left: 0; transform: translateY(-50%);}
    .position-middle-center {top: 50%; left: 50%; transform: translate(-50%, -50%);}
    .position-middle-right {top: 50%; right: 0; transform: translateY(-50%);}
    .position-bottom-left {bottom: 0; left: 0;}
    .position-bottom-center {bottom: 0; left: 50%; transform: translateX(-50%);}
    .position-bottom-right {bottom: 0; right: 0;}

    .banner-content h4 {margin: 15px 0;}
    .banner-1 h4 {margin: 0 0 14px 0;}
    .banner-content .icons {margin: 20px 0 60px;}
    
    .banner-1, .banner-3 {width: 720px;}
    .banner-2 {width: 850px;}
    .banner-2 {margin: 0; text-align: center !important;}
    .banner-1 .text3 {margin-top: 40px;}
    .banner-3 .text3 {margin: 40px 0;}
    .banner-2 .both-btn {margin: auto;}
    .banner-2 h5, .banner-3 h5 {display: none;}
    /* .female .banner-1 h5 {order: 4;} */
    .female .banner-1 .female-customers-content-main {order: 1;margin-bottom: 15px;}
    .female .banner-1 .both-btn {order: 2;}
  }
  
  @media (max-width: 768px) {
    .banner-1 .female-customers-content-main {margin: 13px 0 14px; justify-content: center;}
    .female-customers-content {gap: 5px;}
    .female-customers-text {font-size: 14px;line-height: 9px;}
    .banner-bg {background: linear-gradient(284.21deg, #FFFFFF 2.21%, #F6EDDF 132.6%);}
    .banner-slider-nav {top: 70%;}
    .banner-slider-nav svg {width: 16px; height: 24px;}
    .banner-slider-prev path, .banner-slider-next path {stroke: #414042;}

    .banner-content {text-align: center;}    
    .banner-content h2 {line-height: 34px; font-size: 19px;}
    .banner-content h5 {order: 4; font-weight: 600; font-size: 16px;}
    .banner-content h4 {font-size: 18px;}
    .banner-1 h4 {font-size: 16px;}
    .banner-content .text3 {font-size: 18px;}
    .banner-content .both-btn {order: 5; margin: auto;}

    .banner-1 h4 {margin: 0 0 14px 0; line-height: normal;}
    .icons {margin: 2px 0 16px;}
    .icons p {font-size: 12px;}
    .female .banner-3 h5 { margin-bottom: 10px}
  } 

  @media (max-width: 320px) {
    .banner-2 h2 {font-size: 16px;}
  }
</style>

<script>
  {% comment %} $(document).ready(function(){
    $('.banner-img').slick({
      slidesToShow: 1,
      infinite: true,
      autoplay: true,
      autoplaySpeed: 5000,
      pauseOnHover: false,
      pauseOnFocus: false, 
      {% if section.blocks.size > 1 %}

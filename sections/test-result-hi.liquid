{% style %}
  .result-hi .stage-images {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
  }
  .result-hi .main-stage-section .stage-item {
    padding-top: 100%;
  }
  .result-hi .main-stage-section .stage-item .indicator {
    position: absolute;
    right: 0;
    bottom: 0;
    text-align: center;
    width: 32px;
    height: 32px;
    background: #68aa9e;
    border: 1px solid #fff;
    border-radius: 100%;
  }
  .result-hi p.location, .result-hi p.checkbox {
    max-width: 100px;
    left: 21%;
    transform: translate(-3%);
  }
  .result-hi .main-stage-section .stage-item>p {
    position: absolute;
    right: 0;
    text-align: center;
    display: none;
  }
  .result-hi .main-stage-section .stage-item .indicator span {
    width: 32px;
    height: 32px;
    align-items: center;
    justify-content: center;
    margin: 0 auto;
    display: none;
  }
  .result-hi .main-stage-section .checkStage .checkbox, .result-hi .main-stage-section .selectedStage .location {
    display: flex!important;
  }
  .result-hi .main-stage-section .col-4 {
    display: none;
  }
  .result-hi .main-stage-section .selectedStage, .result-hi .main-stage-section .checkStage  {
    display: block;
  }
  .result-hi .main-stage-section #female-stages, .result-hi .main-stage-section #male-stages {
    flex-direction: row-reverse;
  }
  .result-hi .page-result-hero.male #female-stages, .result-hi .page-result-hero.female #male-stages, .result-hi #male-stages, .result-hi #female-stages {
    display: none;
  }
  .result-hi .page-result-hero.male #male-stages, .result-hi .page-result-hero.female #female-stages {
    display: flex;
  }
  .result-hi .checkStage:after {
    content: "\2192";
    position: absolute;
    top: 60%;
    left: 0;
    font-size: 38px;
    transform: translate(-50%,-50%);
  }
  @media only screen and (max-width: 767.98px) {
    .template-page.result-hi p.location, .template-page.result-hi p.checkbox {
      font-size: 14px;
      max-width: 100px;
      left: 0%;
      transform: translate(10%);
    }
    .result-hi .page-result-hero .main-section .visitor-info h1 {
      font-size: 32px;
    }
  }
{% endstyle %}
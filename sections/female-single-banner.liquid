<!-- Sections -> female-single-banner -->

<div class="container-fluid p-0 female-banner">
  <div class="row m-0">
    <div class="col-12 p-0">
      <div class="position-relative">
        {% if section.settings.desk_img != blank %}
          <img src="{{ section.settings.desk_img | img_url: 'master' }}" class="w-100 d-none d-md-block">
        {% endif %}
        {% if section.settings.mob_img != blank %}
          <img src="{{ section.settings.mob_img | img_url: 'master' }}" class="w-100 d-md-none">
        {% endif %}
        <p class="position-absolute customer">Palak, Trayaâ€™s Customer</p>
      </div>
        
      <div class="d-flex flex-column position-absolute female-banner-content">
        <h2 class="font-weight-bold">{{ section.settings.heading_text }}</h2>
                  
        <div class="d-none d-md-flex mt-4 both-btn">
          {% if section.settings.button_text != blank %}
            <a id="female-hair-test-button2" href="{{ section.settings.button_url }}" class="btn-dark d-flex justify-content-center align-items-center font-weight-bold text-uppercase">{{ section.settings.button_text }}<sup>TM</sup></a>
          {% endif %}

          <span class="myresult m-0 mt-2 m-md-0 ml-md-2">
            <a id="myPlanHome" href="/pages/result4" class="btn-dark d-flex justify-content-center align-items-center font-weight-bold text-uppercase rcmd_plan_btn">My Recommended Plan</a>  
          </span>
        </div>
      </div>

      <div class="female-banner-content-bottom">
        <div class="d-flex align-items-center flex-column flex-md-row px-md-5 pt-md-5 pb-md-4 pb-5 pt-2" style="justify-content: space-evenly;">
          <div class="text-center font-weight-bold bottom-title">Proven Results</div>
          
          <div class="bottom-sub-txt">
            <div>
              <p class="first m-0 text-center d-flex align-items-center justify-content-center">93%</p>
              <p class="second m-0 text-center font-weight-bold">Saw Results*</p>
            </div>
            <div>
              <p class="first m-0 text-center d-flex align-items-center justify-content-center">2.5L
                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="40" height="40" viewBox="0 0 117 117" fill="none">
                  <rect width="117" height="117" fill="url(#pattern0)"/>
                  <defs>
                    <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                      <use xlink:href="#image0_108_43" transform="scale(0.00854701)"/>
                    </pattern>
                    <image id="image0_108_43" width="117" height="117" xlink:href="data:image/png;base64,"/>
                  </defs>
                </svg>
              </p>
              <p class="second m-0 text-center font-weight-bold">Chose Traya</p>
            </div>
            <div>
              <p class="first m-0 text-center d-flex align-items-center justify-content-center">25K
                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="40" height="40" viewBox="0 0 117 117" fill="none">
                  <rect width="117" height="117" fill="url(#pattern0)"/>
                  <defs>
                    <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                      <use xlink:href="#image0_108_43" transform="scale(0.00854701)"/>
                    </pattern>
                    <image id="image0_108_43" width="117" height="117" xlink:href="data:image/png;base64,"/>
                  </defs>
                </svg>
              </p>
              <p class="second m-0 text-center font-weight-bold">Customer Reviews</p>
            </div>
            <div>
              <p class="first m-0 text-center d-flex align-items-center justify-content-center">4.5</p>
              <p class="second m-0 text-center">
                <img src="https://cdn.shopify.com/s/files/1/0100/1622/7394/files/imgpsh_fullsize_anim_1_6111e1dd-5dc1-451a-b3bf-6d903170f448.png?v=1692180837" class="m-auto position-relative" style="width: 100px; bottom: 5px;">
              </p>
            </div>
          </div>

          <div class="d-flex d-md-none flex-column mt-4 both-btn">
            {% if section.settings.button_text != blank %}
              <a id="female-hair-test-button2" href="{{ section.settings.button_url }}" class="btn-dark d-flex justify-content-center align-items-center font-weight-bold text-uppercase">{{ section.settings.button_text }}<sup>TM</sup></a>
            {% endif %}
  
            <span class="myresult m-0 mt-2 m-md-0 ml-md-2">
              <a id="myPlanHome" href="/pages/result4" class="btn-dark d-flex justify-content-center align-items-center font-weight-bold text-uppercase rcmd_plan_btn">My Recommended Plan</a>  
            </span>
          </div>
        </div>
        
        <div class="small-txt"><p>*As per an internal study conducted in December 2022</p></div>
      </div>
      
    </div>
  </div>
</div>

<style>      
  {% if page.handle contains 'femalev2' %}
    #myHeader {background: #414042;}
  {% endif %}

  .female-banner {margin-top: 60px;}
  .female-banner-content {top: 20%; right: 2%;}
  .female-banner-content h2 {font-size: 50px;}
  .both-btn a {width: 300px; height: 50px; border-radius: 9px; font-size: 18px;}
  .female-banner-content-bottom {background: #A59BB8; border-bottom-left-radius: 9px; border-bottom-right-radius: 9px;}
  .bottom-title {font-size: 50px; color: #fff;}
  .bottom-sub-txt {display: grid; grid-template-columns: auto auto auto auto; gap: 50px;}
  .first {font-family: fredoka_oneregular !important; font-size: 50px; color: #fff;}
  .second {font-size: 18px; color: #fff;}
  .small-txt {font-size: 10px; text-align: right; padding: 5px;}
  .small-txt p {color: #fff;}
  .customer {bottom: 15px; left: 16%; font-size: 14px; color: #7B7B7B;}
  
  @media (min-width: 1500px) { 
    .female-banner-content h2 {font-size: 70px;}
    .both-btn a {width: 350px; height: 60px; font-size: 22px;}
    .bottom-title, .first {font-size: 60px;}
    .second {font-size: 30px;}
  }

  @media (min-width: 768px) and (max-width: 1200px) { 
    .female-banner-content {top: 10%; right: 1%;}
    .female-banner-content h2, .bottom-title, .first {font-size: 30px;}
    .both-btn a {width: 200px; font-size: 12px;}
    .first svg {height: 22px; width: 22px;}
    .second {font-size: 14px;}
    .customer {font-size: 10px;}
  }
  
  @media (max-width: 767px) {
    .female-banner {margin-top: 40px;}
    .female-banner-content {top: 0; right: unset; width: 100%; padding: 5px 0;}
    .female-banner-content h2 {font-size: 22px; margin: 10px 0; letter-spacing: 0em; text-align: center;}
    .female-banner-content-bottom {background: #A59BB8; border-radius: 0;}
    .bottom-title {font-size: 22px; padding: 20px 0; line-height: 0;}
    .bottom-sub-txt {grid-template-columns: auto auto; gap: 10px 40px;}
    .first {font-size: 30px;}   
    .first svg {height: 22px; width: 22px;}
    .second {font-size: 12px;}
    .small-txt {position: absolute; top: 90%; right: 5px; transform: translate(-50%, 0) rotate(-90deg); white-space: nowrap;}
    .small-txt p {position: absolute; color: #414042;}
    .customer {bottom: 0; left: 30%; font-size: 10px;}
  } 
</style>

{% schema %}
  {
    "name": "Female Single Banner",
    "settings": [
      {
        "type": "image_picker",
        "id": "desk_img",
        "label": "Desktop Image"
      },
      {
        "type": "image_picker",
        "id": "mob_img",
        "label": "Mobile Image"
      },
      {
        "type": "html",
        "id": "heading_text",
        "label": "Heading Text"
      },
      {
        "type": "text",
        "id": "button_text",
        "label": "Button Text"
      },
      {
        "type": "url",
        "id": "button_url",
        "label": "Button URL"
      }
    ],
    "presets": [{"name": "Female Single Banner"}]
  }
{% endschema %}
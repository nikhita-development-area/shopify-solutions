.order-tracking::before {
	content: '';
    display: block;
    height: 2px;
    width: calc(100% - 15px);
    top: 25px;
    position: absolute;
    left: calc(-50% + 15px);
    z-index: 0;
    border: 2px solid rgba(200, 208, 206, 1);
    border-style: dashed;
}
.order-tracking:first-child:before{display: none;}
.order-tracking.complete_track:before{background-color: #27aa80;}
  

<div class="container order-status d-none" order__status>
 <div class="order_status_login">
  <div class="order-init align-items-center py-3" style="">
      <!-- <img class="lazyload d-md-none" data-src="//cdn.shopify.com/s/files/1/0100/1622/7394/files/order-alert_20x.png"> -->
    <div class="order_satus_design"> 
    <p class="order-status-text">Order Id</p>
      <p class="order__id font-weight-bold" orderid>#1234</p>
    </div>
  <div class="order__stages d-flex"><span class="ml-3 stages"><a href="/" track_url style="text-decoration: underline;">Track here</a></span></div>
  </div>
  <div class="status">
    <div class="order__information">
     
    <div class="order-tracking complete_track order__stages active_status" order_placed>
    <div class="is-complete status__border active_order" order-status-bdr><span class="dots order_login_achieve">{% render 'tracking-svg' %}</span></div>
    <span class="ml-3 stages">Order<br> Placed</span>
</div>
<div class="order-tracking order_dots order__stages" shipping__pending>
    <div class="is-complete status__border" order-status-bdr><span class="dots order_dots"></span></div>
    <span class="log_order_text ml-3 stages">Verified by<br>Doctors</span>
</div>
<div class="order-tracking order_dots order__stages" track_here>
    <div class="is-complete status__border" order-status-bdr><span class="dots order_dots"></span></div>
    <span class="log_order_text ml-3 stages">Order<br> Shipped</span>
</div>
<div class=" order-tracking order_dots order__stages" order__delivered>
    <div class="is-complete status__border" order-status-bdr><span class="dots order_dots"></span></div>
    <span class="log_order_text ml-3 stages">Delivered</span>
</div>
    </div>
  </div>
 </div>
</div>

<script>
  $('.buttons').click(function(){
    var btn = $(this);
    var hidden_btn = btn.parent('.angle__icons').find('.buttons');
    if(hidden_btn.hasClass('d-none')) {
      hidden_btn.removeClass('d-none');
      btn.addClass('d-none');
    }
    else {
      hidden_btn.removeClass('d-none');
    }
    var status = btn.parents('.order-status').find('.status');
    status.fadeToggle();
  });
</script>


<!-- section/product-about -->
<div class="container combo-about">
  <h3 class="combo-title">{{ section.settings.title }}</h3>
  {% for field in product.metafields.custom.about_tab.value %}
    {% assign words = field | split: '->' | first %}
    {% for word in words %}
    <h2 class="combo-subtitle">
      <svg width="19" height="22" viewBox="0 0 14 17" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
        <rect width="14" height="17" fill="url(#pattern0)"/>
        <defs>
          <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
            <use xlink:href="#image0_711_219" transform="translate(-1 -0.719697) scale(0.00952381 0.00757576)"/>
          </pattern>
          <image id="image0_711_219" width="379" height="466" xlink:href="data:image/png;base64,"/>
        </defs>
      </svg>
      {{ word }}
    </h2>
    {% endfor %}
  <h5 class="combo_p_about-content">
    {% assign words = field | split: '->' | last %}
     {% for word in words %}
      {% if word.size > 250 %}
        <span id="less__desc" class="show_less-desc">{{ word | truncate: 250 }}<button class="read_mr_less_btn" id="read_mr_btn" onclick="read_btn">Read more</button></span>
        <span id="more__desc" class="hide_more-desc" style="display: none">{{ word }}<button class="read_mr_less_btn" id="read_ls_btn" onclick="read_btn">Read less</button></span>
      {% else %}
       {{ word }}
      {% endif %}
    {% endfor %}
  </h5>
  {% endfor %}
</div>
<div class="container hr-div"> </div>
{% schema %}
  {
    "name": "Product About",
    "settings": [
      {
        "type": "text",
        "id": "title",
        "label": "Title",
        "default": "What Do I Get?"
      },
      {
        "type": "range",
        "id": "margin_top",
        "min": -10,
        "max": 90,
        "step": 1,
        "unit": "px",
        "label": "Margin from above section",
        "default": 48
      }
    ],
    "presets": [
      {
        "name": "Product About"
      }
    ]
  }
{% endschema %}

<style>
  @media (min-width: 768px){
    .combo-about{
      margin-top: {{ section.settings.margin_top }}px;
    }
  }
</style>
<script>
  $(document).ready(function(){
    $('.read_mr_less_btn').click(function(){
      $(this).parent().hide();
      $(this).parent().siblings().show();
    });
  });
</script>
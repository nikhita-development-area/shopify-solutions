{% style %}
  .stage1-new-testimonial {margin-bottom: 15px !important;padding-bottom: 0 !important;}
  /* .our-customers-img img {height: auto !important; width: auto !important;} */
  .stage1-new-testimonial .slick-slider {margin:0 -15px;}
  .stage1-new-testimonial .slick-slide {padding:10px;margin-right:15px;margin-left:15px;margin-bottom: 15px;margin-top: 10px;}
  {% if page.handle == 'female-result' or page.handle == 'result-fe' %}
    .result-product a {pointer-events: none;}
  {% endif %}
  .cstm_result-hi {display: none;}
  .product-item > .row li {list-style: inherit;}
  {% if page.handle == 'result-hi' %}
    .cstm_english {display: none;} 
    .cstm_result-hi {display: block;} 
  {% endif %}
  {% if page.handle == 'result5' %}
    .price-on-top {background: #68aa9e;border-radius: 0 12px;}
    .price-on-top h5, .price-on-top h6 {color: #fff;}
  {% endif %}
  {% if template.suffix == 'resultco' or page.handle == 'result' or page.handle == 'result-male' %}
    .co_bdr {border-radius: 0.7rem;}
    .price-on-top {background-color: #68AA9E;border-radius: 5px;}
    .price-on-top h5 {color: #fff;}
    @media (max-width: 767.98px) {
      .section-title.size_heading_result {font-size: 22px;}
      .male_pro_rating {display:none;}
    }
  {% endif %}
  .result4_pd_image {height: 100%;}
  .verified_icon {position: absolute;top: 9%;left: 3%;transform: translate(0,-10%);}
  .result4 .must-have, .result4c .must-have {position: absolute;z-index: 1;right: 0.2%;transform: translate(0, 0);}
  .male_pro_badges ul {margin-bottom: 0;}
  .result_product_slides .slick-dots li button{font-size: 15px;text-align: center;display: flex;align-items: center;color: #000;font-weight: bold;}
  .result_product_slides .slick-dots li.slick-active button:before, .result_product_slides .slick-dots li button:before{content:unset;}
  .result_product_slides .slick-dots li.slick-active button{color: #fff;display: flex;justify-content: center;align-items: center;width: 25px;height: 25px;background: #B7D340;border-radius: 50%;}
  @media only screen and (min-width: 767.98px) {
    .male_pro_rating p img {max-width: 150px !important;}
    p.male_pro_dosage {margin-top: 15px;margin-bottom: 15px;}
    .male_product_list_flex {display: flex;align-items: center;}
  }
  .male_prod_tit {padding:0px !important;}
  .male_pro_rating {position: absolute;top: 0;right: 0;}
  p.male_pro_dosage {text-align: center;width: 100%;font-size: 20px;color: #3F3F3F;}
  ul.cstm_english{display: flex;justify-content: space-evenly;color: #3F3F3F;font-weight: 700;font-size: 18px;border-top: 1.00067px solid #b7d3404a;border-bottom: 1.00067px solid #b7d3404a;padding: 15px 0;width:100%;margin-top:25px;}
  span.ml_benefits_bdr {padding-left: 15px;color:#b7d3404a;}
  .male_pro_badges ul {display: flex;justify-content: space-between;}
  ul.cstm_english li:last-child span.ml_benefits_bdr {display: none;}
  .male_pro_rating p img {max-width: 120px;}
 .result4 .product-item {border: 1.11057px solid #B7D340 !important;border-radius: 10.078px;padding:15px !important;}
 .result4 .product-item, .result4c .product-item {border: 1.11057px solid #D4D4D4 !important;border-radius: 10.078px;padding:15px !important;}
  ul.cstm_english li {position: relative;text-align: center;}
  .result4c .result_product_slides:not(.slick-initialized) > .col-12:first-of-type ~ * {display: none}
  .result4 .result_product_slides .male_prod_tit h4, .result4c .result_product_slides .male_prod_tit h4 {font-size: 14px !important; line-height: 18x; color: #3F3F3F; min-height: 36px; margin: 0 !important;}
  .result4 .result_product_slides .price-on-top .text-tradewind, .result4c .result_product_slides .price-on-top .text-tradewind {font-size: 16px !important; line-height: 16px; letter-spacing: 0em; color: #3F3F3F; margin: 10px 0 !important;}
  .result4 .result_product_slides .col-12 p.m-0.py-2.text-center, .result4c .result_product_slides .col-12 p.m-0.py-2.text-center {font-size: 12px; line-height: 13px; color: #3F3F3F; opacity: 0.8; min-height: 42px;}
  .result4 .cstm-ingre, .result4c .cstm-ingre {font-size: 14px; line-height: 9px; color: #3F3F3F; opacity: 0.8;}
  .result4 .cstm-dosage-4c, .result4c .cstm-dosage-4c {font-size: 14px; line-height: 10px; color: #3F3F3F; opacity: 0.8; margin-top: 10px !important; min-height: 10px;}
  .result4 .result_card_ingredient_name, .result4c .result_card_ingredient_name {font-size: 11px; font-weight: 600; line-height: 12px; min-height: 44px;}
  .result4 .cstm-dosage-icons, .result4c .cstm-dosage-icons {gap: 10px; flex-wrap: nowrap !important;}
  .result4 .must-have svg, .result4c .must-have svg {height: inherit; width: 90px;}
  .result4 .result_product_slides .slick-dots {display: none;}
  
  @media only screen and (max-width: 767.98px) {
    /* .male_pro_badges ul li svg {width: 90px;} */
    /* .result4 .male_pro_badges ul li svg, .result4c .male_pro_badges ul li svg {height: inherit;} */
    ul.cstm_english {margin-left: 10px;margin-right: 10px;padding-left: 0 !important;padding-right: 0 !important;}
    .male_pro_badges ul {margin-bottom: 0;}
    img.male_pro_img {height: 100%;}
    .male_pro_badges img {max-width: 80px;}
    .male_prod_tit h6 {width: 100% !important;margin: 0 auto !important;padding: 0 !important;margin-bottom: 10px !important;text-align: center;}
    .price-on-top {text-align: center;width: 100%;}
    .col-12.col-md-7.gender-m-stage-1-pro.pl-0.pl-md-3 {padding: 0 !important;}
    ul.cstm_english {text-align: center;}
    ul.cstm_english {text-align: center !important;font-size: 11px !important;padding: 10px 5px !important;}
    .male_pro_rating {display:none;}
    .result4 .result_card_ingredient_name, .result4c .result_card_ingredient_name {min-height: 45px;font-size: 9px !important;}
    .result4 .must-have, .result4c .must-have {right: -1.2%;}
    .result4 .result_product_slides .price-on-top .text-tradewind, .result4c .result_product_slides .price-on-top .text-tradewind {margin: 0!important;}
    .verified_icon svg {width: 50px;height: 50px;}
    .verified_icon {top: 11%;}
    .result4 .m-stage-1-Pro_title {font-size: 24px;}
  }
  @media only screen and (max-width: 400px) {
    .result4 .result_product_slides .col-12 p.m-0.py-2.text-center, .result4c .result_product_slides .col-12 p.m-0.py-2.text-center {font-size: 10px;}

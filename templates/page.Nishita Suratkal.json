    "919c899e-89ea-4d1d-b172-fa7da8970f60": {
      "type": "QA-accordion",
      "blocks": {
        "d7070a17-13db-4a0f-9269-1d09a6021139": {
          "type": "faq",
          "settings": {
            "Question": "What is a Dietician?",
            "Answer": "A Dietician is an expert in food and nutrition who helps in treatments in Lifestyle & diet changes that will help treat hair and health."
          }
        },
        "66ff8a23-deb4-47ef-b521-e5d78326633f": {
          "type": "faq",
          "settings": {
            "Question": "Why do patients visit Nishita for?",
            "Answer": "Patients frequently visit Nishita for hyper-acidity, menstrual disorders, anxiety, and clinical depression."
          }
        },
        "739bc52f-c51e-4cb7-8c8f-df10a0436e0c": {
          "type": "faq",
          "settings": {
            "Question": "What is Nishita Suratkal’s education qualification?",
            "Answer": "Nishita has a BSc.,MSc. in Dietetics. She is a qualified Nutritionist and Dietitian."
          }
        }

    "treatment_options_WUzAC8": {
      "type": "treatment_options",
      "blocks": {
        "text_CXMcqF": {
          "type": "text",
          "settings": {
            "image": "shopify:\/\/shop_images\/treatment5.png",
            "image_text": "Platelet-Rich Plasma <br> (PRP):",
            "para": "Involves injecting your platelets into the scalp to stimulate hair growth."
          }
        },
        "text_3a4EXT": {
          "type": "text",
          "settings": {
            "image": "shopify:\/\/shop_images\/treatment7.png",
            "image_text": "Stem Cell Therapy:",
            "para": "A newer technique injecting stem cells into the scalp to encourage hair follicles to grow."
          }
        },
        "text_bM3mPF": {
          "type": "text",
          "settings": {
            "image": "shopify:\/\/shop_images\/treatment6.png",
            "image_text": "Hair Transplant: ",
            "para": "Surgical procedures like Follicular Unit Transplantation (FUT) or Follicular Unit Extraction (FUE) involve transplanting hair from one part of the scalp to the balding areas."
          }
        },
        "text_j3cN6d": {
          "type": "text",
          "settings": {
            "image": "shopify:\/\/shop_images\/treatment2.png",
            "image_text": "Medication:",
            "para": "Oral medications like Finasteride (for men) and topical treatments like Minoxidil can slow hair loss and promote growth."
          }
        },
        "text_bLUqbi": {
          "type": "text",
          "settings": {
            "image": "shopify:\/\/shop_images\/treatment3.png",
            "image_text": "Topical Treatments: ",
            "para": "These include solutions and foams applied directly to the scalp. Minoxidil is the most commonly effective in both men and women."
          }
        }
      },
